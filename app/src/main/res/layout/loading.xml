<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<RelativeLayout
		android:id="@+id/program_details"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<RelativeLayout
			android:id="@+id/content_container"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content">

			<View
				android:id="@+id/_bg__icon"
				android:layout_width="102dp"
				android:layout_height="102dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp" />


			<RelativeLayout
				android:id="@+id/group_14"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0.00091552734375dp">


				<RelativeLayout
					android:id="@+id/group_4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp">


					<View
						android:id="@+id/rectangle_1"
						android:layout_width="102dp"
						android:layout_height="102dp"
						android:layout_marginStart="0dp"
						android:layout_marginTop="0dp"
						android:background="@drawable/rectangle_1_shape"/>

				</RelativeLayout>

				<RelativeLayout
					android:id="@+id/group_5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="8dp"
					android:layout_marginTop="8dp">

					<ImageView
						android:id="@+id/LTCorner"
						android:layout_width="24.791748046875dp"
						android:layout_height="23.773223876953dp"
						android:layout_marginStart="0dp"
						android:layout_marginTop="-0dp"
						android:contentDescription="@string/lefttopcorner"
						android:src="@drawable/ltcornerw" />

					<ImageView
						android:id="@+id/LBCorner"
						android:layout_width="23.773620605469dp"
						android:layout_height="24.792175292969dp"
						android:layout_marginStart="0dp"
						android:layout_marginTop="63dp"
						android:contentDescription="@string/leftbottomcorner"
						android:src="@drawable/lbcornerw" />

					<ImageView
						android:id="@+id/RBCorner"
						android:layout_width="24.792175292969dp"
						android:layout_height="23.773620605469dp"
						android:layout_marginStart="63dp"
						android:layout_marginTop="64dp"
						android:contentDescription="@string/rightbottomcorner"
						android:src="@drawable/rbcornerw" />

					<ImageView
						android:id="@+id/RTCorner"
						android:layout_width="23.774047851562dp"
						android:layout_height="24.792144775391dp"
						android:layout_marginStart="64dp"
						android:layout_marginTop="-0dp"
						android:contentDescription="@string/righttopcorner"
						android:src="@drawable/rtcornerw" />

				</RelativeLayout>


				<RelativeLayout
					android:id="@+id/group_12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp">

					<ImageView
						android:id="@+id/skelaton"
						android:layout_width="102.98910522461dp"
						android:layout_height="102.98907470703dp"
						android:layout_marginStart="0dp"
						android:layout_marginTop="0dp"
						android:contentDescription="Skeleton"
						android:src="@drawable/skelaton" />

				</RelativeLayout>

			</RelativeLayout>

		</RelativeLayout>

	</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>